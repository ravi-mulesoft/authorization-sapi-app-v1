#%RAML 1.0 DataType

type: object
properties:
  msg:
    required: true
    type: string
    
  code:
    required: true
    type: integer
    
  success:
    required: true
    type: boolean
    example: true
  error:
    required: true
    type: object
    properties:
      'type':
        required: true
        type: string
        
      title:
        required: true
        type: string
        
      details:
        required: true
        type: string
         